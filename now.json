{
  "version": 2,
  "name": "dockerhub-stat-tracker",
  "builds": [
    {
      "src": "api/libraries/public/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/images/details",
      "dest": "api/images/public/images.js",
      "methods": ["GET"],
      "headers": {
        "content-type": "application/json"
      }
    },
    {
      "src": "/api/images/summary",
      "dest": "api/images/public/summary.js",
      "methods": ["GET"],
      "headers": {
        "content-type": "application/json"
      }
    },
    {
      "src": "/api/images/downloads/alltime",
      "dest": "api/images/public/downloads.alltime.js",
      "methods": ["GET"],
      "headers": {
        "content-type": "application/json"
      }
    },
    {
      "src": "/api/images/downloads/monthly",
      "dest": "api/images/public/downloads.monthly.js",
      "methods": ["GET"],
      "headers": {
        "content-type": "application/json"
      }
    }
  ]
}
